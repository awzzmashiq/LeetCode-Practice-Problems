class Solution {
    public boolean isPalindrome(int x) {

        if(x<0)
        return false;

        int sum=0;
        int n=x;

        while(x!=0)
        {
            int pop= x%10;
            x=x/10;
            sum=sum*10+pop;
        }
        
        return sum==n;


        
    }
}
